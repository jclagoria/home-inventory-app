{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Company",
  "description": "The Company",
  "required": [
    "name",
    "email",
    "address_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the address."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of Company."
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The logo url of the company."
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The city description."
    },
    "website_url": {
      "$id": "#/properties/website_url",
      "type": "string",
      "title": "The city website_url."
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The street address."
    },
    "address_id": {
      "$id": "#/properties/address_id",
      "type": "integer",
      "title": "The zipcode."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the company."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the company was last updated."
    },
    "delete_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the State was last updated."
    }
  }
}
