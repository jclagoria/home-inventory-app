{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Country",
  "description": "The Country",
  "required": [
    "name",
    "code"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the Country."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the Country."
    },
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The code of the Country."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the country."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the country was last updated."
    },
    "delete_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the State was last updated."
    }
  }
}
