{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Item_Type",
  "description": "The Item_Type",
  "required": [
    "name"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the item type."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of item type."
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The city name."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    }
  }
}
